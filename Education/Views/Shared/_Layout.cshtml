    <script>
        $(function () {
            $('#subcribe-mail').submit(function () {
                var email = $(this).find("input").val();
                console.log(email);
                $.ajax({
                    type: "POST",
                    url: "/Home/Subcribe/",
                    data: { email: email },
                    success: function (data) {
                        if (data.StatusCode == 200) {
                            alert(data.Message);
                            $(".message-subcribe").text("");
                        } else {
                            $(".message-subcribe").text(data.Message);
                        }
                    }
                })
            })
           
        })
    </script>
