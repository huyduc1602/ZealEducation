<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from preschool.dreamguystech.com/html-template/blank-page.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 05 Aug 2021 01:52:28 GMT -->
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <title>@ViewBag.Title - Zeal Education</title>

    <link rel="shortcut icon" href="~/Areas/Admin/Content/assets/img/favicon.png">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,500;0,600;0,700;1,400&amp;display=swap">

    <link rel="stylesheet" href="~/Areas/Admin/Content/assets/plugins/bootstrap/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/Areas/Admin/Content/assets/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Content/assets/plugins/fontawesome/css/all.min.css">
    @RenderSection("plugincss", required: false)
    <link rel="stylesheet" href="~/Areas/Admin/Content/assets/css/style.css">
    @RenderSection("css", required: false)
</head>
<body>

    <div class="main-wrapper">

        <div class="header">

            <div class="header-left">
                <a href="/Admin/Dashboard" class="logo">
                    <img src="~/Areas/Admin/Content/assets/img/logo-admin.png" alt="Logo">
                </a>
                <a href="/Admin/Dashboard" class="logo logo-small">
                    <img src="~/Areas/Admin/Content/assets/img/logo-admin-small.png" alt="Logo" width="30" height="30">
                </a>
            </div>

            <a href="javascript:void(0);" id="toggle_btn">
                <i class="fas fa-align-left"></i>
            </a>

            <div class="top-nav-search">
                <form>
                    <input type="text" class="form-control" placeholder="Search here">
                    <button class="btn" type="submit"><i class="fas fa-search"></i></button>
                </form>
            </div>


            <a class="mobile_btn" id="mobile_btn">
                <i class="fas fa-bars"></i>
            </a>


            <ul class="nav user-menu">

                <li class="nav-item dropdown noti-dropdown">
                    <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown">
                        <i class="far fa-bell"></i> <span class="badge badge-pill">3</span>
                    </a>
                    <div class="dropdown-menu notifications">
                        <div class="topnav-dropdown-header">
                            <span class="notification-title">Notifications</span>
                            <a href="javascript:void(0)" class="clear-noti"> Clear All </a>
                        </div>
                        <div class="noti-content">
                            <ul class="notification-list">
                                <li class="notification-message">
                                    <a href="#">
                                        <div class="media">
                                            <span class="avatar avatar-sm">
                                                <img class="avatar-img rounded-circle" alt="User Image" src="~/Areas/Admin/Content/assets/img/profiles/avatar-02.jpg">
                                            </span>
                                            <div class="media-body">
                                                <p class="noti-details"><span class="noti-title">Carlson Tech</span> has approved <span class="noti-title">your estimate</span></p>
                                                <p class="noti-time"><span class="notification-time">4 mins ago</span></p>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <li class="notification-message">
                                    <a href="#">
                                        <div class="media">
                                            <span class="avatar avatar-sm">
                                                <img class="avatar-img rounded-circle" alt="User Image" src="~/Areas/Admin/Content/assets/img/profiles/avatar-11.jpg">
                                            </span>
                                            <div class="media-body">
                                                <p class="noti-details"><span class="noti-title">International Software Inc</span> has sent you a invoice in the amount of <span class="noti-title">$218</span></p>
                                                <p class="noti-time"><span class="notification-time">6 mins ago</span></p>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <li class="notification-message">
                                    <a href="#">
                                        <div class="media">
                                            <span class="avatar avatar-sm">
                                                <img class="avatar-img rounded-circle" alt="User Image" src="~/Areas/Admin/Content/assets/img/profiles/avatar-17.jpg">
                                            </span>
                                            <div class="media-body">
                                                <p class="noti-details"><span class="noti-title">John Hendry</span> sent a cancellation request <span class="noti-title">Apple iPhone XR</span></p>
                                                <p class="noti-time"><span class="notification-time">8 mins ago</span></p>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <li class="notification-message">
                                    <a href="#">
                                        <div class="media">
                                            <span class="avatar avatar-sm">
                                                <img class="avatar-img rounded-circle" alt="User Image" src="~/Areas/Admin/Content/assets/img/profiles/avatar-13.jpg">
                                            </span>
                                            <div class="media-body">
                                                <p class="noti-details"><span class="noti-title">Mercury Software Inc</span> added a new product <span class="noti-title">Apple MacBook Pro</span></p>
                                                <p class="noti-time"><span class="notification-time">12 mins ago</span></p>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="topnav-dropdown-footer">
                            <a href="#">View all Notifications</a>
                        </div>
                    </div>
                </li>


                <li class="nav-item dropdown has-arrow">
                    <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown">
                        <span class="user-img"><img class="rounded-circle" src="~/Areas/Admin/Content/assets/img/profiles/avatar-01.jpg" width="31" alt="Ryan Taylor"></span>
                    </a>
                    <div class="dropdown-menu">
                        <div class="user-header">
                            <div class="avatar avatar-sm">
                                <img src="~/Areas/Admin/Content/assets/img/profiles/avatar-01.jpg" alt="User Image" class="avatar-img rounded-circle">
                            </div>
                            <div class="user-text">
                                <h6>@Session["FullName"]</h6>
                                <p class="text-muted mb-0">@Session["UserName"]</p>
                            </div>
                        </div>
                        <a class="dropdown-item" href="/Admin/Users/Details/@Session["UserId"]">My Profile</a>
                        <a class="dropdown-item" href="/Admin/Dashboard/Logout">Logout</a>
                    </div>
                </li>

            </ul>

        </div>


        <div class="sidebar" id="sidebar">
            <div class="sidebar-inner slimscroll">
                <div id="sidebar-menu" class="sidebar-menu">
                    <ul>
                        <li class="menu-title">
                            <span>Main Menu</span>
                        </li>
                        <li>
                            <a href="/Admin/Dashboard"><i class="fas fa-th-large"></i> <span>Dashboard</span></a>
                        </li>
                        @if (null != Session["CandicateId"])
                        {
                            <li class="submenu">
                                <a href="#"><i class="fas fa-user-graduate"></i> <span>personal information</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("information", "DetailsCandicate", "Candicates")</li>
                                </ul>
                            </li>
                        }
                        else if (null != Session["FaultyId"])
                        {
                            <li class="submenu">
                                <a href="#"><i class="fas fa-user-graduate"></i> <span>List of teaching classes</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("List of teaching classes", "DetailsFaulty", "Faulties")</li>
                                </ul>
                            </li>
                            <li class="submenu">
                                <a href="#"><i class="fas fa-user-graduate"></i> <span> Learning Info</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Learning List", "Index", "LearningInfo")</li>
                                </ul>
                            </li>
                        }
                        else
                        {
                            <li class="submenu">
                                <a href="#"><i class="fas fa-user-graduate"></i> <span> Candicates</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Candicate List", "Index", "Candicates")</li>
                                    <li>@Html.ActionLink("Candicate Add", "Create", "Candicates")</li>
                                </ul>
                            </li>
                            <li class="submenu">
                                <a href="#"><i class="fas fa-user-graduate"></i> <span> Learning Info</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Learning List", "Index", "LearningInfo")</li>
                                </ul>
                            </li>
                            <li class="submenu">
                                <a href="#"><i class="fas fa-book-reader"></i> <span> Batchs</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Batch List", "Index", "Batch")</li>
                                    <li>@Html.ActionLink("Batch Add", "Create", "Batch")</li>
                                </ul>
                            </li>
                            <li class="submenu">
                                <a href="#"><i class="fas fa-chalkboard-teacher"></i> <span> Fautlies</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Fautly List", "Index", "Faulties")</li>
                                    <li>@Html.ActionLink("Fautly Add", "Create", "Faulties")</li>
                                </ul>
                            </li>
                            <li class="submenu">
                                <a href="#"><i class="fas fa-layer-group"></i> <span> Courses</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Course List", "Index", "Courses")</li>
                                    <li>@Html.ActionLink("Course Add", "Create", "Courses")</li>
                                </ul>
                            </li>
                            <li class="submenu">
                                <a href="#"><i class="fas fa-clipboard-list"></i> <span> Exams</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Exam List", "Index", "Exams")</li>
                                    <li>@Html.ActionLink("Exam Add", "Create", "Exams")</li>
                                </ul>
                            </li>
                            <li class="submenu">
                                <a href="#"><i class="fas fa-blog"></i> <span> Blogs</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Blog List", "Index", "Blogs")</li>
                                    <li>@Html.ActionLink("Blog Add", "Add", "Blogs")</li>
                                    <li>@Html.ActionLink("Category List", "Index", "Categories")</li>
                                    <li>@Html.ActionLink("Category Add", "Create", "Categories")</li>
                                </ul>
                            </li>
                            <li class="submenu">
                                <a href="#"><i class="fas fa-user-circle"></i> <span> Users</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("User List", "Index", "Users")</li>
                                    <li>@Html.ActionLink("User Add", "Create", "Users")</li>
                                    <li>@Html.ActionLink("Group User List", "Index", "GroupUsers")</li>
                                    <li>@Html.ActionLink("Group User Add", "Create", "GroupUsers")</li>
                                    <li>@Html.ActionLink("Grant Permisson", "GrantPermission", "GroupUsers")</li>
                                </ul>
                            </li>

                            <li class="submenu">
                                <a href="#"><i class="fas fa-layer-group"></i> <span> Report</span> <span class="menu-arrow"></span></a>
                                <ul>
                                    <li>@Html.ActionLink("Student By Month And Year", "ReportStudentByMonthAndYear", "Report")</li>
                                    <li>@Html.ActionLink("Still Go Back To School", "StillGoBackToSchool", "Report")</li>
                                    <li>@Html.ActionLink("Student By Class Room", "ReportStudentByClassRoom", "Report")</li>
                                    <li>@Html.ActionLink("Complete Certificate", "CompleteCertificate", "Report")</li>
                                    <li>@Html.ActionLink("The Class Has Ended", "TheClassHasEnded", "Report")</li>
                                    <li>@Html.ActionLink("Classes Are Starting", "ClassesAreStarting", "Report")</li>
                                    <li>@Html.ActionLink("Remaining Tuition", "RemainingTuition", "Report")</li>
                                </ul>
                            </li>
                        }
                    </ul>
                </div>
            </div>
        </div>


        <div class="page-wrapper">
            <div class="content container-fluid">

                @RenderBody()
            </div>
        </div>

    </div>


    <script src="~/Areas/Admin/Content/assets/js/jquery-3.5.1.min.js"></script>

    <script src="~/Areas/Admin/Content/assets/js/popper.min.js"></script>
    <script src="~/Areas/Admin/Content/assets/plugins/bootstrap/js/bootstrap.min.js"></script>

    <script src="~/Areas/Admin/Content/assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    @RenderSection("pluginjs", required: false)
    <script src="~/Areas/Admin/Content/assets/js/script.js"></script>
    @RenderSection("scripts", required: false)
</body>

<!-- Mirrored from preschool.dreamguystech.com/html-template/blank-page.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 05 Aug 2021 01:52:28 GMT -->
</html>